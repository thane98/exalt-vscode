{
    "name": "exalt-scripting",
    "displayName": "Exalt (FE) Scripting",
    "description": "Support for Exalt, a scripting language for various FE games",
    "repository": "https://github.com/thane98/exalt-vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "exalt",
            "aliases": ["Exalt", "exalt"],
            "extensions": ["exl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "exalt",
            "scopeName": "source.exalt",
            "path": "./syntaxes/exalt.tmLanguage.json"
        }]
    }
}